<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.akashraj.moviemanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MovieActivity"
            android:label="@string/movies"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".DistributerActivity"
            android:label="@string/distributers"
            android:parentActivityName=".MainActivity"
            />
        <activity android:name=".TheatreActivity"
            android:label="@string/theatres"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>